#! /bin/sh
channel=$1
if [ $channel -lt 15 ]
then
    echo "calcmd 1 0 8 0 1 0 2 $channel" > /sys/devices/qdrv/control
else
    echo "calcmd 1 0 8 0 1 1 2 $channel" > /sys/devices/qdrv/control
fi

echo "calcmd 6 0 8 0 1 0 2 0" > /sys/devices/qdrv/control

	echo "calcmd 5 0 10 0 1 0 2 0 3 0" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 0 2 $2 3 $3 4 $4 5 $5 $6" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 1 2 $7 3 $8 4 $9 5 $10 $11" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 2 2 $12 3 $13 4 $14 5 $15 $16" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 3 2 $17 3 $18 4 $19 5 $20 $21" > /sys/devices/qdrv/control
		echo "calcmd 46 0 10 0 1 $22 $23 2 $24 $25" > /sys/devices/qdrv/control

	echo "calcmd 5 0 10 0 1 0 2 0 3 2" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 0 2 $26 3 $27 4 $28 5 $29 $30" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 1 2 $31 3 $32 4 $33 5 $34 $35" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 2 2 $36 3 $37 4 $38 5 $39 $40" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 3 2 $41 3 $42 4 $43 5 $44 $45" > /sys/devices/qdrv/control
		echo "calcmd 46 0 10 0 1 $46 $47 2 $48 $49" > /sys/devices/qdrv/control

	echo "calcmd 5 0 10 0 1 1 2 0 3 0" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 0 2 $50 3 $51 4 $52 5 $53 $54" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 1 2 $55 3 $56 4 $57 5 $58 $59" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 2 2 $60 3 $61 4 $62 5 $63 $64" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 3 2 $65 3 $66 4 $67 5 $68 $69" > /sys/devices/qdrv/control
		echo "calcmd 46 0 10 0 1 $70 $71 2 $72 $73" > /sys/devices/qdrv/control

	echo "calcmd 5 0 10 0 1 1 2 0 3 2" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 0 2 $74 3 $75 4 $76 5 $77 $78" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 1 2 $79 3 $80 4 $81 5 $82 $83" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 2 2 $84 3 $85 4 $86 5 $87 $88" > /sys/devices/qdrv/control
		echo "calcmd 45 0 15 0 1 3 2 $89 3 $90 4 $91 5 $92 $93" > /sys/devices/qdrv/control
		echo "calcmd 46 0 10 0 1 $94 $95 2 $96 $97" > /sys/devices/qdrv/control

echo "calcmd 5 0 10 0 1 2 2 0 3 0" > /sys/devices/qdrv/control
echo "calcmd 45 0 4 0" > /sys/devices/qdrv/control
echo "calcmd 46 0 4 0" > /sys/devices/qdrv/control
